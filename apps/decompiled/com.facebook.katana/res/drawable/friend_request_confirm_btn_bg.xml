<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_pressed="true">
        <shape>
            <solid android:color="@color/fbui_btn_light_primary_bg_pressed" />
            <stroke android:width="1.0dip" android:color="@color/fbui_btn_light_primary_stroke_pressed" />
            <corners android:radius="2.0dip" />
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape>
            <solid android:color="@color/fbui_btn_light_primary_bg_default" />
            <stroke android:width="1.0dip" android:color="@color/fbui_btn_light_primary_stroke_enabled" />
            <corners android:radius="2.0dip" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/fbui_btn_light_primary_bg_default" />
            <stroke android:width="1.0dip" android:color="@color/fbui_btn_light_primary_stroke_disabled" />
            <corners android:radius="2.0dip" />
        </shape>
    </item>
</selector>