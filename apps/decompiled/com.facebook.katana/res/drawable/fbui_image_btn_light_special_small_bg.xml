<?xml version="1.0" encoding="utf-8"?>
<selector android:variablePadding="false"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_pressed="true">
        <shape android:shape="oval">
            <solid android:color="@color/fbui_btn_light_special_bg_pressed" />
            <stroke android:width="1.0dip" android:color="@color/fbui_btn_light_special_bg_pressed" />
            <size android:height="@dimen/fbui_image_button_size_small" android:width="@dimen/fbui_image_button_size_small" />
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape android:shape="oval">
            <solid android:color="@color/fbui_btn_light_special_bg_enabled" />
            <stroke android:width="1.0dip" android:color="@color/fbui_btn_light_special_bg_enabled" />
            <size android:height="@dimen/fbui_image_button_size_small" android:width="@dimen/fbui_image_button_size_small" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="oval">
            <solid android:color="@color/fbui_btn_light_special_bg_disabled" />
            <stroke android:width="1.0dip" android:color="@color/fbui_btn_light_special_bg_disabled" />
            <size android:height="@dimen/fbui_image_button_size_small" android:width="@dimen/fbui_image_button_size_small" />
        </shape>
    </item>
</selector>