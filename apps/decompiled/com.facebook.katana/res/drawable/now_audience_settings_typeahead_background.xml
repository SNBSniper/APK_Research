<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:left="-1.0px" android:top="-1.0px" android:right="-1.0px">
        <selector>
            <item android:state_enabled="true" android:state_pressed="true">
                <shape android:shape="rectangle">
                    <solid android:color="@color/now_divebar_medium_background_color" />
                    <stroke android:width="1.0px" android:color="@color/now_divebar_divider_color" />
                </shape>
            </item>
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="@color/now_divebar_dark_background_color" />
                    <stroke android:width="1.0px" android:color="@color/now_divebar_divider_color" />
                </shape>
            </item>
        </selector>
    </item>
</layer-list>